{
  "compilerOptions": {
      "outDir": "./dist/out-tsc",
    "module": "commonjs",
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "moduleResolution": "node",
      "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
      "assets/dist",
      "node_modules",
      "typings"
  ]
}
